{
  "expo": {
    "name": "kaamwale3",
    "slug": "kaamwale3",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "kaamwale3",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.atulsharma897438.kaamwale3",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "package": "com.atulsharma897438.kaamwale3",
      "permissions": ["INTERNET"],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "googleServicesFile": "./google-services.json"
    },

    "plugins": [
      "expo-router",
      "@maplibre/maplibre-react-native",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#667eea",
          "sounds": ["./assets/notification.wav"]
        }
      ],
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "eas": {
        "projectId": "8a6c9f14-6f97-4d37-8daf-a8e8a86487cb"
      }
    },

    "owner": "atul-dowork"
  }
}
