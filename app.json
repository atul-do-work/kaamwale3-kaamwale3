{
  "expo": {
    "name": "kaamwale3",
    "slug": "do-work-neweas",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "kaamwale3",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.atulsharma897438.kaamwale3",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "package": "com.atulsharma897438.kaamwale3",
      "permissions": ["INTERNET", "POST_NOTIFICATIONS", "RECEIVE_BOOT_COMPLETED"],
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      }
    },

    "plugins": [
      "expo-router",
      "@maplibre/maplibre-react-native",
      "expo-notifications",
      "expo-web-browser",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "eas": {
        "projectId": "12636e4e-571b-42e8-a75e-30e0e8d7bd32"
      }
    },

    "owner": "atulkumar20962"
  }
}
